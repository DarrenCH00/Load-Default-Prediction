    "from pyspark.sql.functions import col\n",
    "\n",
    "columns_to_convert = ['app_data_flag_mobile', 'app_data_flag_emp_phone', 'app_data_flag_work_phone', 'app_data_flag_cont_mobile', 'app_data_flag_phone', 'app_data_flag_email']\n",
    "\n",
    "for column in columns_to_convert:\n",
    "    df_combined_final = df_combined_final.withColumn(column, col(column) == 1)\n",
    "    \n",
    "column_types = df_combined_final.dtypes\n",
    "\n",
    "\n",
    "numerical_columns = [column for column, dtype in column_types if dtype in ['int', 'double', 'float', 'long']]\n",
    "\n",
    "categorical_columns = [column for column, dtype in column_types if dtype == 'string']\n",
    "\n",
    "boolean_columns = [column for column, dtype in column_types if dtype == 'boolean']\n",
    "\n",
    "df_combined_final.select(numerical_columns).describe().show()\n",
    "\n",
    "for col in categorical_columns:\n",
    "    df_combined_final.groupBy(col).count().orderBy('count', ascending=False).show(5)\n",
    "        \n",
    "for col in boolean_columns:\n",
    "    df_combined_final.groupBy(col).count().show()\n",
    "\n"
   ]
