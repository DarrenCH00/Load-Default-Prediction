   "source": [
    "from pyspark.ml.feature import StringIndexer, OneHotEncoder, VectorAssembler, StandardScaler\n",
    "from pyspark.ml.clustering import KMeans\n",
    "from pyspark.ml.evaluation import ClusteringEvaluator\n",
    "from pyspark.ml import Pipeline\n",
    "\n",
    "features_df = df_combined_final.select('app_data_gender','app_data_credit_score_1','app_data_credit_score_2','app_data_credit_score_3','app_data_income_total','app_data_loan_to_income_ratio','total_credit_to_income_ratio','app_data_average_credit','num_of_prev_app','num_of_approved_app','app_data_own_car','app_data_own_property')\n",
    "\n",
    "cols = features_df.columns\n",
    "\n",
    "numericInputCols = ['app_data_credit_score_1','app_data_credit_score_2','app_data_credit_score_3','app_data_income_total','app_data_loan_to_income_ratio','total_credit_to_income_ratio','app_data_average_credit','num_of_prev_app','num_of_approved_app']\n",
    "categoryInputCols = ['app_data_gender','app_data_own_car','app_data_own_property']\n",
    "categoryCols = categoryInputCols\n",
    "outputCols = [f'{x}_index' for x in categoryCols]\n",
    "\n",
    "indexer = StringIndexer(inputCols=categoryCols, outputCols=outputCols)\n",
    "\n",
    "inputCols_OHE = [x for x in outputCols]  # Output columns from StringIndexer\n",
    "outputCols_OHE = [f'{x}_vec' for x in outputCols]  # Corresponding one-hot encoded output columns\n",
    "\n",
    "encoder = OneHotEncoder(inputCols=inputCols_OHE, outputCols=outputCols_OHE)\n",
    "\n",
    "assemblerInput = outputCols_OHE + numericInputCols\n",
    "assembler = VectorAssembler(inputCols=assemblerInput, outputCol='features')\n",
    "\n",
    "# Define StandardScaler to scale the features\n",
    "scaler = StandardScaler(inputCol='features', outputCol='scaledFeatures')\n",
    "\n",
    "# Define KMeans model\n",
    "k_means = KMeans(featuresCol='scaledFeatures', k=2)\n",
    "\n",
    "# Create the pipeline\n",
    "pipeline = Pipeline(stages=[indexer, encoder, assembler, scaler, k_means])\n",
    "\n",
    "# Fit the pipeline model\n",
    "pipelineModel = pipeline.fit(features_df)\n",
    "\n",
    "# Transform the data\n",
    "clustered_df = pipelineModel.transform(features_df)\n",
    "\n",
    "evaluator = ClusteringEvaluator()\n",
    "\n",
    "silhouette = evaluator.evaluate(clustered_df)\n",
    "print(\"Silhouette with squared euclidean distance = \" + str(silhouette))\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 25,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "No of clusters: 2 Silhouette Score: -0.1478351775877484\n",
      "No of clusters: 3 Silhouette Score: -0.3161749120631777\n",
      "No of clusters: 4 Silhouette Score: -0.3772776447704308\n",
      "No of clusters: 5 Silhouette Score: -0.21564834755877563\n",
      "No of clusters: 6 Silhouette Score: -0.23272244256271904\n",
      "No of clusters: 7 Silhouette Score: -0.3974254429969557\n",
      "No of clusters: 8 Silhouette Score: -0.44049480579924566\n",
      "No of clusters: 9 Silhouette Score: -0.45754104123822387\n"
     ]
    }
   ],
   "source": [
    "#Here we are taking the data transformed by the StandardScaler\n",
    "silhouette_arr=[]\n",
    "for k in range(2,10):\n",
    "    k_means= KMeans(featuresCol='scaledFeatures', k=k)\n",
    "    pipeline = Pipeline(stages=[indexer, encoder, assembler, scaler, k_means])\n",
    "    pipelineModel = pipeline.fit(features_df)\n",
    "    clustered_df = pipelineModel.transform(features_df)\n",
    "    silhouette = evaluator.evaluate(clustered_df)\n",
    "    silhouette_arr.append(silhouette)\n",
    "    print('No of clusters:',k,'Silhouette Score:',silhouette)"
   ]
  },
