   "source": [
    "from pyspark.sql.functions import col, count, sum\n",
    "from pyspark.sql import functions as F\n",
    "\n",
    "df_app_data = df_app_data.select([col(c).alias('app_data_' + c) for c in df_app_data.columns])\n",
    "\n",
    "df_prev_data = df_prev_data.select([col(c).alias('prev_data_' + c) for c in df_prev_data.columns])\n",
    "\n",
    "df_combined = df_app_data.join(df_prev_data, df_app_data.app_data_id_app == df_prev_data.prev_data_id_app, how='inner') \n",
    "\n",
    "agg_df = df_prev_data.groupBy(\"prev_data_id_app\").agg(count('*').alias('num_of_prev_app'))\n",
    "\n",
    "filtered_df_prev_data = df_prev_data.filter(df_prev_data.prev_data_name_contract_status == \"Approved\")\n",
    "\n",
    "agg_df2 = filtered_df_prev_data.groupBy(\"prev_data_id_app\").agg(count('*').alias('num_of_approved_app'))\n",
    "\n",
    "agg_df = agg_df.select(agg_df[\"prev_data_id_app\"].alias(\"agg_df_id_app\"), \"num_of_prev_app\")\n",
    "agg_df2 = agg_df2.select(agg_df2[\"prev_data_id_app\"].alias(\"agg_df2_id_app\"), \"num_of_approved_app\")\n",
    "\n",
    "agg_join = agg_df.join(agg_df2, agg_df.agg_df_id_app == agg_df2.agg_df2_id_app, how='inner')\n",
    "\n",
    "df_combined_agg = df_combined.join(agg_join, df_combined.app_data_id_app == agg_join.agg_df_id_app, how = 'left')\n",
    "\n",
    "df_combined_agg = df_combined_agg.filter(df_prev_data.prev_data_name_contract_status == \"Approved\")\n",
    "\n",
    "total_credit = df_combined_agg.groupBy(\"app_data_id_app\").agg(sum(df_app_data.app_data_amt_credit).alias(\"total_credit\"))\n",
    "\n",
    "total_credit = total_credit.select(total_credit[\"app_data_id_app\"].alias(\"total_credit_id_app\"), \"total_credit\")\n",
    "\n",
    "df_combined_agg1 = df_combined_agg.join(total_credit, df_combined.app_data_id_app == total_credit.total_credit_id_app, how = 'left')\n",
    "\n",
    "df_combined_agg1 = df_combined_agg1.withColumn('total_credit_to_income_ratio',df_combined_agg1.total_credit/df_combined_agg1.app_data_income_total)\n",
    "\n",
    "four_cols = df_combined_agg1.select(\"app_data_id_app\",'num_of_prev_app','num_of_approved_app','total_credit','total_credit_to_income_ratio')\n",
    "\n",
    "four_cols.show()\n"
   ]
  },
