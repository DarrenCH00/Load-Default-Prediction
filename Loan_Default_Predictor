    "from pyspark.ml.classification import RandomForestClassifier\n",
    "\n",
    "rf = RandomForestClassifier(featuresCol = 'features', labelCol = 'label', numTrees = 10)\n",
    "gbt = GBTClassifier(featuresCol = 'features', labelCol = 'label')"
