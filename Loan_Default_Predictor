   "source": [
    "#plot 1: Default in relation to age\n",
    "\n",
    "df_plot_1 = df_combined_final.select('app_data_age','load_default_is_default')\n",
    "\n",
    "df_plot_1 = df_plot_1.groupBy(\"app_data_age\", \"load_default_is_default\").agg(count(\"*\").alias(\"total_count\"))\\\n",
    "             .orderBy(\"app_data_age\",'load_default_is_default')\n",
    "\n",
    "\n",
    "import matplotlib.pyplot as plt\n",
    "import seaborn as sns\n",
    "from pyspark.sql import SparkSession\n",
    "\n",
    "pdf = df_plot_1.toPandas()\n",
    "\n",
    "plt.figure(figsize=(10, 6))\n",
    "sns.barplot(x='app_data_age', y='total_count', hue='load_default_is_default', data=pdf)\n",
    "\n",
    "plt.title('Count of Loan Defaults by Age')\n",
    "plt.xlabel('Age')\n",
    "plt.ylabel('Count')\n",
    "plt.xticks(rotation=45)\n",
    "plt.tight_layout()\n",
    "\n",
    "plt.show()\n"
   ]
  },
