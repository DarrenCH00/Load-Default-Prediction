    "import pandas as pd\n",
    "\n",
    "numerical_columns = [column for column, dtype in column_types if dtype in ['int', 'double', 'float', 'long']]\n",
    "\n",
    "df_numerical = df_combined_final.select(numerical_columns)\n",
    "\n",
    "df_numerical_pd = df_numerical.toPandas()\n",
    "\n",
    "for column in numerical_columns:\n",
    "    percentile_25 = df_numerical_pd[column].quantile(0.25)\n",
    "    percentile_50 = df_numerical_pd[column].quantile(0.50)  \n",
    "    percentile_75 = df_numerical_pd[column].quantile(0.75)\n",
    "\n",
    "    print(f\"Column: {column}\")\n",
    "    print(f\"  25th percentile: {percentile_25}\")\n",
    "    print(f\"  50th percentile (median): {percentile_50}\")\n",
    "    print(f\"  75th percentile: {percentile_75}\\n\")\n",
    "\n",
    "\n"
   ]
